{
  "name": "@dwk/dwk.io",
  "version": "1.0.0",
  "scripts": {
    "build": "NODE_OPTIONS=--experimental-strip-types eleventy --config=eleventy.config.ts",
    "postbuild": "NODE_OPTIONS=--experimental-strip-types node scripts/postbuild.ts",
    "start": "NODE_OPTIONS=--experimental-strip-types eleventy --config=eleventy.config.ts --serve",
    "typecheck": "tsc --noEmit && tsc --noEmit --project worker/tsconfig.json",
    "deploy": "npm run build && wrangler deploy",
    "dev:worker": "wrangler dev",
    "cf-typegen": "wrangler types",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [],
  "author": {
    "name": "David W. Keith",
    "email": "git@dwk.io",
    "url": "https://dwk.io/"
  },
  "license": "ISC",
  "description": "DWK's Cyber Home",
  "type": "module",
  "devDependencies": {
    "@dwk/eleventy-shared": "*",
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "@cloudflare/vitest-pool-workers": "^0.12.13",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^25.2.3",
    "cheerio": "^1.2.0",
    "dotenv": "^17.3.1",
    "eleventy-plugin-gen-favicons": "^1.1.3",
    "html-minifier-terser": "^7.2.0",
    "jsonld-lint": "^0.3.0",
    "openpgp": "^6.3.0",
    "typescript": "^5.9.3",
    "vitest": "~3.2.0",
    "wrangler": "^4.66.0"
  }
}
